<style type="text/css">
@import url('https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,200;1,100&display=swap');

.contain {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100vh;
    animation: animate 16s ease-in-out infinite;  
    background-repeat: no-repeat;
    background-size: cover;  
}

@keyframes animate {
